{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigsSchema",
  "type": "object",
  "required": [
    "id",
    "date",
    "module",
    "title",
    "key",
    "value",
    "signature",
    "provenance",
    "created_at"
  ],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^CFG-[0-9]{8}-[0-9]{4}$"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "module": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "key": {
      "type": "string"
    },
    "value": {
      "type": "string"
    },
    "signature": {
      "type": "string"
    },
    "provenance": {
      "type": "object"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
